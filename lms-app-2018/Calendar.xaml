					  Stretch="Uniform"
					  Stroke="{Binding Foreground, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Button}}}" />
			</Button>
			<Button Command="Delete"
					Style="{DynamicResource CrystalDrawingButton}"
					ToolTip="Delete Assignment">
