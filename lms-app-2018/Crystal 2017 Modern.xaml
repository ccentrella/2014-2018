    <!--Set template for crystal chrome buttons. These are advanced, and disappear when disabled.-->
    <SolidColorBrush x:Uid="SolidColorBrush_14"
                     x:Key="ChromeButtonForeground"
                     Color="RoyalBlue" />
    <SolidColorBrush x:Uid="SolidColorBrush_15"
                     x:Key="ChromeButtonBackground"
                     Color="White" />
    <ControlTemplate x:Uid="ControlTemplate_2"
                     TargetType="{x:Type Button}"
                     x:Key="CrystalChromeButton">
        <!--Advanced-->
        <Grid x:Uid="Grid_2"
              ClipToBounds="{TemplateBinding ClipToBounds}">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Uid="VisualStateGroup_1"
                                  x:Name="CommonStates">
                    <VisualStateGroup.Transitions>
                        <VisualTransition x:Uid="VisualTransition_4"
                                          GeneratedDuration="0:0:0.5" />
                        <VisualTransition x:Uid="VisualTransition_5"
                                          To="Pressed"
                                          GeneratedDuration="0" />
                        <VisualTransition x:Uid="VisualTransition_6"
                                          From="Pressed"
                                          GeneratedDuration="0" />
                    </VisualStateGroup.Transitions>
                    <VisualState x:Uid="VisualState_1"
                                 x:Name="Normal" />
                    <VisualState x:Uid="VisualState_2"
                                 x:Name="MouseOver">
                        <Storyboard x:Uid="Storyboard_6">
                            <DoubleAnimation x:Uid="DoubleAnimation_6"
                                             Storyboard.TargetName="Background"
                                             Storyboard.TargetProperty="Opacity"
                                             To="0.925" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Uid="VisualState_3"
                                 x:Name="Pressed">
                        <Storyboard x:Uid="Storyboard_7">
                            <DoubleAnimation x:Uid="DoubleAnimation_7"
                                             Storyboard.TargetName="Background"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Uid="FocusStates"
                                  x:Name="FocusStates">
                    <VisualState x:Uid="Unfocused"
                                 x:Name="Unfocused" />
                    <VisualState x:Uid="Focused"
                                 x:Name="Focused">
                        <Storyboard x:Uid="Storyboard_8">
                            <DoubleAnimation x:Uid="DoubleAnimation_8"
                                             Storyboard.TargetName="Background"
                                             Storyboard.TargetProperty="Opacity"
                                             From="0.8"
                                             To="0.95"
                                             AutoReverse="True"
                                             RepeatBehavior="Forever">
                                <DoubleAnimation.EasingFunction>
                                    <QuadraticEase x:Uid="QuadraticEase_3"
                                                   EasingMode="EaseInOut" />
                                </DoubleAnimation.EasingFunction>
                            </DoubleAnimation>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Uid="Border_1"
                    x:Name="Background"
                    Opacity="0.8"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    CornerRadius="0"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    Background="{TemplateBinding Background}" />
            <ContentPresenter x:Uid="ContentPresenter_1"
                              RecognizesAccessKey="True"
                              Margin="{TemplateBinding Padding}"
                              x:Name="Content"
                              HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                              VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
        </Grid>
        <ControlTemplate.Triggers>
            <!--If the button is disabled, it will disappear.-->
            <Trigger x:Uid="Trigger_2"
                     Property="IsEnabled"
                     Value="False">
                <Setter x:Uid="Setter_1"
                        Property="Visibility"
                        Value="Collapsed" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <Style x:Uid="Style_1"
           TargetType="{x:Type Button}"
           BasedOn="{StaticResource {x:Type Button}}"
           x:Key="CrystalCloseButton">
        <Setter x:Uid="Setter_2"
                Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_3"
                                 TargetType="{x:Type Button}">
                    <!--Advanced-->
                    <Grid x:Uid="Grid_3"
                          ClipToBounds="{TemplateBinding ClipToBounds}">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Uid="VisualStateGroup_2"
                                              x:Name="CommonStates">
                                <VisualStateGroup.Transitions>
                                    <VisualTransition x:Uid="VisualTransition_7"
                                                      GeneratedDuration="0:0:0.5" />
                                    <VisualTransition x:Uid="VisualTransition_8"
                                                      To="Pressed"
                                                      GeneratedDuration="0" />
                                    <VisualTransition x:Uid="VisualTransition_9"
                                                      From="Pressed"
                                                      GeneratedDuration="0" />
                                </VisualStateGroup.Transitions>
                                <VisualState x:Uid="VisualState_4"
                                             x:Name="Normal" />
                                <VisualState x:Uid="VisualState_5"
                                             x:Name="MouseOver">
                                    <Storyboard x:Uid="Storyboard_9">
                                        <ColorAnimation x:Uid="ColorAnimation_1"
                                                        Storyboard.TargetName="brush1"
                                                        Storyboard.TargetProperty="Color"
                                                        To="DarkRed" />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Uid="VisualState_6"
                                             x:Name="Pressed">
                                    <Storyboard x:Uid="Storyboard_10">
                                        <ColorAnimation x:Uid="ColorAnimation_2"
                                                        Storyboard.TargetName="brush1"
                                                        Storyboard.TargetProperty="Color"
                                                        To="Black" />
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Uid="VisualStateGroup_3"
                                              x:Name="FocusStates">
                                <VisualState x:Uid="VisualState_7"
                                             x:Name="Unfocused" />
                                <VisualState x:Uid="VisualState_8"
                                             x:Name="Focused">
                                    <Storyboard x:Uid="Storyboard_11">
                                        <DoubleAnimation x:Uid="DoubleAnimation_9"
                                                         Storyboard.TargetName="Background"
                                                         Storyboard.TargetProperty="Opacity"
                                                         From="0.8"
                                                         To="0.95"
                                                         AutoReverse="True"
                                                         RepeatBehavior="Forever">
                                            <DoubleAnimation.EasingFunction>
                                                <QuadraticEase x:Uid="QuadraticEase_4"
                                                               EasingMode="EaseInOut" />
                                            </DoubleAnimation.EasingFunction>
                                        </DoubleAnimation>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border x:Uid="Border_2"
                                x:Name="Background"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                CornerRadius="0"
                                BorderThickness="{TemplateBinding BorderThickness}">
                            <Border.Background>
                                <SolidColorBrush x:Uid="brush1"
                                                 x:Name="brush1"
                                                 Color="Red" />
                            </Border.Background>
                        </Border>
                        <ContentPresenter x:Uid="ContentPresenter_2"
                                          RecognizesAccessKey="True"
                                          Margin="{TemplateBinding Padding}"
                                          x:Name="Content"
                                          HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                          VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <!--If the button is disabled, it will disappear.-->
                        <Trigger x:Uid="Trigger_3"
                                 Property="IsEnabled"
                                 Value="False">
                            <Setter x:Uid="Setter_3"
                                    Property="Visibility"
                                    Value="Collapsed" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--Set template for crystal icon buttons. By default, these have a transparent border and background.-->
    <Style x:Uid="Style_2"
           TargetType="{x:Type Button}"
           x:Key="CrystalIconButton">
        <Setter x:Uid="Setter_4"
                Property="Background"
                Value="Transparent" />
        <Setter x:Uid="Setter_5"
                Property="BorderBrush"
                Value="Transparent" />
        <Setter x:Uid="Setter_6"
                Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_4"
                                 TargetType="{x:Type Button}">
                    <Grid x:Uid="MainGrid"
                          x:Name="MainGrid"
                          ClipToBounds="False">
                        <Border x:Uid="Border_3"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}">
                            <ContentPresenter x:Uid="ContentPresenter_3"
                                              RecognizesAccessKey="True"
                                              Margin="{TemplateBinding Padding}"
                                              x:Name="Content"
                                              HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                              VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
                        </Border>
                        <Ellipse x:Uid="AnimationEllipse"
                                 x:Name="AnimationEllipse"
                                 HorizontalAlignment="Center"
                                 VerticalAlignment="Center"
                                 Height="{TemplateBinding ActualHeight}"
                                 Width="{TemplateBinding ActualWidth}"
                                 Fill="Black"
                                 Opacity="0"
                                 RenderTransformOrigin="0.5,0.5">
                            <Ellipse.RenderTransform>
                                <ScaleTransform x:Uid="ScaleTransform_1"
                                                ScaleX="0.15"
                                                ScaleY="0.15" />
                            </Ellipse.RenderTransform>
                            <Ellipse.CacheMode>
                                <BitmapCache x:Uid="BitmapCache_1"
                                             RenderAtScale="2" />
                            </Ellipse.CacheMode>
                        </Ellipse>

                    </Grid>

                    <ControlTemplate.Triggers>
                        <Trigger x:Uid="Trigger_4"
                                 Property="IsPressed"
                                 Value="True">
                            <Setter x:Uid="Setter_7"
                                    Property="Effect">
                                <Setter.Value>
                                    <DropShadowEffect x:Uid="DropShadowEffect_1"
                                                      Color="{TemplateBinding Background}"
                                                      ShadowDepth="0.2" />
                                </Setter.Value>
                            </Setter>
                        </Trigger>

                        <!--While the mouse hovers over the button, there will be a drop shadow and a circle.-->
                        <Trigger x:Uid="Trigger_5"
                                 Property="IsMouseOver"
                                 Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard x:Uid="BeginStoryboard_1">
                                    <Storyboard x:Uid="Storyboard_12">
                                        <DoubleAnimation x:Uid="DoubleAnimation_10"
                                                         Duration="0:0:0.25"
                                                         Storyboard.TargetName="AnimationEllipse"
                                                         Storyboard.TargetProperty="Opacity"
                                                         To="0.4" />
                                        <DoubleAnimation  x:Uid="DoubleAnimation_11"
                                                          Duration="0:0:0.25"
                                                          Storyboard.TargetName="AnimationEllipse"
                                                          Storyboard.TargetProperty="RenderTransform.ScaleX"
                                                          To="1.6" />
                                        <DoubleAnimation  x:Uid="DoubleAnimation_12"
                                                          Duration="0:0:0.25"
                                                          Storyboard.TargetName="AnimationEllipse"
                                                          Storyboard.TargetProperty="RenderTransform.ScaleY"
                                                          To="1.6" />

                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard x:Uid="BeginStoryboard_2">
                                    <Storyboard x:Uid="Storyboard_13">
                                        <DoubleAnimation  x:Uid="DoubleAnimation_13"
                                                          Duration="0:0:0.25"
                                                          Storyboard.TargetName="AnimationEllipse"
                                                          Storyboard.TargetProperty="Opacity"
                                                          To="0" />
                                        <DoubleAnimation  x:Uid="DoubleAnimation_14"
                                                          Duration="0:0:0.25"
                                                          Storyboard.TargetName="AnimationEllipse"
                                                          Storyboard.TargetProperty="RenderTransform.ScaleX"
                                                          To="1" />
                                        <DoubleAnimation  x:Uid="DoubleAnimation_15"
                                                          Duration="0:0:0.25"
                                                          Storyboard.TargetName="AnimationEllipse"
                                                          Storyboard.TargetProperty="RenderTransform.ScaleY"
                                                          To="1" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>

                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--Set the template for crystal drawing buttons, which change colors when hovering over them or pressing them.-->
    <Style TargetType="{x:Type ButtonBase}"
           x:Key="CrystalDrawingButton">
        <Setter Property="Background"
                Value="Transparent" />
        <Setter Property="Foreground"
                Value="RoyalBlue" />
        <Setter Property="BorderBrush"
                Value="Transparent" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ButtonBase}">
                    <Grid x:Name="MainGrid"
                          ClipToBounds="False">
                        <Border Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}">
