        <Setter x:Uid="Setter_474"
                Property="Background"
                Value="Transparent" />
        <Setter x:Uid="Setter_475"
                Property="BorderBrush"
                Value="Transparent" />
        <Setter x:Uid="Setter_476"
                Property="BorderThickness"
                Value="1" />
        <Setter x:Uid="Setter_477"
                Property="Padding"
                Value="1" />
