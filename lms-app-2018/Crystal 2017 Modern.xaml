                                                                    <VisualState x:Uid="VisualState_42"
                                                                                 x:Name="MouseOver">
                                                                        <Storyboard x:Uid="Storyboard_48">
                                                                            <DoubleAnimation x:Uid="DoubleAnimation_41"
                                                                                             Storyboard.TargetName="Arrow"
                                                                                             Storyboard.TargetProperty="Opacity"
                                                                                             To="0.5" />
                                                                        </Storyboard>
