    <!--Set properties for ProgressBars-->
    <SolidColorBrush x:Uid="SolidColorBrush_41"
                     x:Key="ProgressBar.Progress"
                     Color="DarkViolet" />
    <SolidColorBrush x:Uid="SolidColorBrush_42"
                     x:Key="ProgressBar.Background"
                     Color="#32E6E6E6" />
    <SolidColorBrush x:Uid="SolidColorBrush_43"
                     x:Key="ProgressBar.Border"
                     Color="#32BCBCBC" />
    <CornerRadius x:Uid="CornerRadius_1"
                  x:Key="ProgressBarCornerRadius"
                  BottomLeft="15"
                  BottomRight="15"
                  TopLeft="15"
                  TopRight="15" />
    <Style x:Uid="Style_44"
           TargetType="{x:Type ProgressBar}">
        <Setter x:Uid="Setter_320"
                Property="Foreground"
                Value="{StaticResource ProgressBar.Progress}" />
        <Setter x:Uid="Setter_321"
                Property="Background"
                Value="{StaticResource ProgressBar.Background}" />
        <Setter x:Uid="Setter_322"
                Property="BorderBrush"
                Value="{StaticResource ProgressBar.Border}" />
        <Setter x:Uid="Setter_323"
                Property="BorderThickness"
                Value="1" />
        <Setter x:Uid="Setter_324"
                Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_38"
                                 TargetType="{x:Type ProgressBar}">
                    <Grid x:Uid="TemplateRoot"
                          x:Name="TemplateRoot">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Uid="VisualStateGroup_21"
                                              x:Name="CommonStates">
                                <VisualState x:Uid="Determinate"
                                             x:Name="Determinate" />
                                <VisualState x:Uid="Indeterminate"
                                             x:Name="Indeterminate">
                                    <Storyboard x:Uid="Storyboard_58"
                                                RepeatBehavior="Forever">
                                        <DoubleAnimationUsingKeyFrames x:Uid="DoubleAnimationUsingKeyFrames_1"
                                                                       Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)"
                                                                       Storyboard.TargetName="Animation">
                                            <EasingDoubleKeyFrame x:Uid="EasingDoubleKeyFrame_1"
                                                                  KeyTime="0"
                                                                  Value="0.25" />
                                            <EasingDoubleKeyFrame x:Uid="EasingDoubleKeyFrame_2"
                                                                  KeyTime="0:0:1"
                                                                  Value="0.25" />
                                            <EasingDoubleKeyFrame x:Uid="EasingDoubleKeyFrame_3"
                                                                  KeyTime="0:0:2"
                                                                  Value="0.25" />
                                        </DoubleAnimationUsingKeyFrames>
                                        <PointAnimationUsingKeyFrames x:Uid="PointAnimationUsingKeyFrames_1"
                                                                      Storyboard.TargetProperty="(UIElement.RenderTransformOrigin)"
                                                                      Storyboard.TargetName="Animation">
                                            <EasingPointKeyFrame x:Uid="EasingPointKeyFrame_1"
                                                                 KeyTime="0"
                                                                 Value="-0.5,0.5" />
                                            <EasingPointKeyFrame x:Uid="EasingPointKeyFrame_2"
                                                                 KeyTime="0:0:1"
                                                                 Value="0.5,0.5" />
                                            <EasingPointKeyFrame x:Uid="EasingPointKeyFrame_3"
                                                                 KeyTime="0:0:2"
                                                                 Value="1.5,0.5" />
                                        </PointAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border x:Uid="Border_24"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                CornerRadius="{StaticResource ProgressBarCornerRadius}"
                                Background="{TemplateBinding Background}" />
                        <Rectangle x:Uid="PART_Track"
                                   x:Name="PART_Track" />
                        <Grid x:Uid="PART_Indicator"
                              x:Name="PART_Indicator"
                              HorizontalAlignment="Left"
                              ClipToBounds="true">
                            <Border x:Uid="Indicator"
                                    x:Name="Indicator"
                                    CornerRadius="{StaticResource ProgressBarCornerRadius}"
                                    Background="{TemplateBinding Foreground}" />
                            <Border x:Uid="Animation"
                                    x:Name="Animation"
                                    CornerRadius="{StaticResource ProgressBarCornerRadius}"
                                    RenderTransformOrigin="0.5,0.5"
                                    Background="{TemplateBinding Foreground}">
                                <Border.RenderTransform>
                                    <TransformGroup x:Uid="TransformGroup_4">
                                        <ScaleTransform x:Uid="ScaleTransform_3" />
                                        <SkewTransform x:Uid="SkewTransform_1" />
                                        <RotateTransform x:Uid="RotateTransform_5" />
                                        <TranslateTransform x:Uid="TranslateTransform_1" />
                                    </TransformGroup>
                                </Border.RenderTransform>
                            </Border>
                        </Grid>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger x:Uid="Trigger_64"
                                 Property="Orientation"
                                 Value="Vertical">
                            <Setter x:Uid="Setter_325"
                                    Property="LayoutTransform"
                                    TargetName="TemplateRoot">
                                <Setter.Value>
                                    <RotateTransform x:Uid="RotateTransform_6"
                                                     Angle="-90" />
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                        <Trigger x:Uid="Trigger_65"
                                 Property="IsIndeterminate"
                                 Value="true">
                            <Setter x:Uid="Setter_326"
                                    Property="Visibility"
                                    TargetName="Indicator"
                                    Value="Collapsed" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
