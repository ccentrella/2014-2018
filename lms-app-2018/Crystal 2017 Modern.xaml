    <!--Set properties for crystal modern bar buttons. These have a flat border on the bottom, and are useful
	 when one wants to dock the buttons on the edge.-->
    <Style x:Uid="Style_6"
           TargetType="{x:Type Button}"
           x:Key="CrystalModernBarButton"
           BasedOn="{StaticResource {x:Type Button}}">
        <Setter x:Uid="Setter_21"
                Property="BorderBrush"
                Value="{StaticResource CrystalBorder2}" />
        <Setter x:Uid="Setter_22"
                Property="BorderThickness"
                Value="{DynamicResource CrystalBorderThickness2}" />
        <Setter x:Uid="Setter_23"
                Property="Width"
                Value="Auto" />
        <Setter x:Uid="Setter_24"
                Property="Background"
                Value="{StaticResource CrystalBackgroundFill2}" />
        <Setter x:Uid="Setter_25"
                Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_6"
                                 TargetType="{x:Type Button}">
                    <!--Basic-->
                    <Grid x:Uid="Grid_4"
                          ClipToBounds="{TemplateBinding ClipToBounds}">
                        <Grid.RowDefinitions>

                            <RowDefinition x:Uid="TopBarRowDefinition"
                                           Height="Auto"
                                           x:Name="TopBarRowDefinition" />
                            <RowDefinition x:Uid="MainRowDefinition"
                                           x:Name="MainRowDefinition" />
                            <RowDefinition x:Uid="BottomBarRowDefinition"
                                           Height="Auto"
                                           x:Name="BottomBarRowDefinition" />
                        </Grid.RowDefinitions>
                        <ContentPresenter x:Uid="ContentPresenter_5"
                                          RecognizesAccessKey="True"
                                          Grid.Row="1"
                                          Margin="{TemplateBinding Padding}"
                                          x:Name="Content"
                                          HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                          VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
                        <Border x:Uid="Border_5"
                                x:Name="Background"
                                Height="8"
                                Grid.Row="0"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Background="{TemplateBinding Background}" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <!--If the button is disabled, it will disappear-->
                        <Trigger x:Uid="Trigger_8"
                                 Property="IsEnabled"
                                 Value="False">
                            <Setter x:Uid="Setter_26"
                                    Property="Visibility"
                                    Value="Collapsed" />
                        </Trigger>

                        <!--While the button is pressed, it's size will be reduced, in addition to a beautiful drop shadow.-->
                        <Trigger x:Uid="Trigger_9"
                                 Property="IsPressed"
                                 Value="True">
                            <Setter x:Uid="Setter_27"
                                    Property="Effect">
                                <Setter.Value>
                                    <DropShadowEffect x:Uid="DropShadowEffect_2"
                                                      Color="{TemplateBinding Background}"
                                                      ShadowDepth="0.5" />
                                </Setter.Value>
                            </Setter>
                            <Setter x:Uid="Setter_28"
                                    Property="RenderTransform">
                                <Setter.Value>
                                    <ScaleTransform x:Uid="ScaleTransform_2"
                                                    ScaleX=".95"
                                                    ScaleY=".95" />
                                </Setter.Value>
                            </Setter>
                        </Trigger>

                        <!--While the mouse hovers over the button, the background will slowly fade.-->
                        <Trigger x:Uid="Trigger_10"
                                 Property="IsMouseOver"
                                 Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard x:Uid="HoverStoryboard"
                                                 x:Name="HoverStoryboard">
                                    <Storyboard x:Uid="Storyboard_14"
                                                Storyboard.TargetName="Background"
                                                Storyboard.TargetProperty="Opacity">
                                        <DoubleAnimation x:Uid="DoubleAnimation_16"
                                                         From="1"
                                                         To="0.4"
                                                         Duration="0:0:0.5" />
                                    </Storyboard>
                                </BeginStoryboard>

                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard x:Uid="RemoveHoverStoryboard"
                                                 x:Name="RemoveHoverStoryboard">
                                    <Storyboard x:Uid="Storyboard_15"
                                                Storyboard.TargetName="Background"
                                                Storyboard.TargetProperty="Opacity">
                                        <DoubleAnimation x:Uid="DoubleAnimation_17"
                                                         From="0.4"
                                                         To="1"
                                                         Duration="0:0:0.5" />
                                    </Storyboard>
                                </BeginStoryboard>

                            </Trigger.ExitActions>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

