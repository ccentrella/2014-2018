<Window x:Class="RecordPro.Ad"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		Title="Ad"
		Icon="pack://application:,,,/Record Pro Logo.png"
		WindowStartupLocation="CenterOwner"
		Background="{DynamicResource CrystalWindowFill}"
		ResizeMode="NoResize"
		Padding="15"
		Width="400"
		Height="250"
		Loaded="Window_Loaded">
	<Grid x:Uid="Grid_1">
		<Grid.RowDefinitions>
			<RowDefinition x:Uid="RowDefinition_4" />
			<RowDefinition Height="30" />
		</Grid.RowDefinitions>
		<Frame x:Uid="MainFrame"
			   Background="{DynamicResource CrystalWindowFill}"
			   x:Name="MainFrame"
			   JournalOwnership="UsesParentJournal"
			   NavigationUIVisibility="Hidden" />
		<Button Style="{DynamicResource CrystalDialogButton}"
				Content="_Got It"
				Grid.Row="1"
				HorizontalAlignment="Right"
				Click="Button_Click"
				Margin="3" />
	</Grid>
</Window>
